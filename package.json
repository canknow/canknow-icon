{
  "name": "canknow-icon",
  "version": "7.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bootstrap": "npm config set registry https://registry.npm.taobao.org && npm install",
    "initialize-icon": "node build/bin/initialize-icon.js",
    "copy-icon-to-example": "node scripts/post-install.js",
    "build": "vue-cli-service build --target lib ./src/index.js",
    "serve:example": "npm run copy-icon-to-example && npm run initialize-icon && vue-cli-service serve ./examples/src/main.js",
    "build:example": "npm run copy-icon-to-example && npm run initialize-icon && vue-cli-service build --dest ./examples/dist ./examples/src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "vue-cli-service lint src",
    "lint:fix": "vue-cli-service lint --fix src",
    "tag": "git push --follow-tags",
    "major": "npm version major && npm run tag",
    "minor": "npm version minor && npm run tag",
    "patch": "npm version patch && npm run tag",
    "tree": "tree  > list.txt",
    "release": "npm publish --registry http://registry.npmjs.org",
    "postinstall": "node scripts/post-install.js"
  },
  "files": [
    "src",
    "build",
    "scripts"
  ],
  "repository": {
    "type": "git",
